package com.lookcapp.strategy;

public class SelectionSorter implements Sorter{

	@Override
	public void sort(Comparable[] data) {
		for (int i = 0; i < data.length; i++) {
			int min = i;
			for (int j = 0; j < data.length; j++) {
				if(data[min].compareTo(data[j]) > 0) {
					min = j;
				}
			}
		}
	}

}
